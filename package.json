{
  "name": "typed",
  "version": "4.0.0",
  "description": "Fast, tiny and type-safe runtime validation library.",
  "main": "dist/typed.js",
  "module": "dist/typed.mjs",
  "typings": "dist/typed.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "benchmark": "npm run build && node ./benchmark",
    "build": "rimraf dist && rollup -c",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "test": "jest"
  },
  "keywords": [
    "array",
    "assert",
    "check",
    "checker",
    "collection",
    "data",
    "error",
    "inference",
    "interface",
    "invalid",
    "joi",
    "json",
    "list",
    "model",
    "object",
    "schema",
    "superstruct",
    "type",
    "typescript",
    "valid",
    "validate",
    "validation",
    "validation",
    "validator"
  ],
  "license": "MIT",
  "author": "brielov <brielov@icloud.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brielov/typed.git"
  },
  "bugs": {
    "url": "https://github.com/brielov/typed/issues"
  },
  "homepage": "https://github.com/brielov/typed#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.27",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "benchmark": "^2.1.4",
    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "jest-diff": "^28.0.1",
    "lint-staged": "^12.4.0",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "superstruct": "^0.15.4",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "zod": "^3.14.4"
  }
}
